<div class="container">
  <div class="timeline">
    <div class="timeline-side">
    </div>
    <div class="timeline-cards">
      <% @timeline.each do |timeline_event| %>
        <%= link_to timeline_event[:memory] do %>
          <%= render "timeline_card", timeline_event: timeline_event %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
